<!-- $Id$ -->
<link rel="stylesheet" type="text/css" media="screen" href="<tmpl_var debug_root>/debug.css">
<script language="Javascript" type="text/javascript" src="<tmpl_var debug_root>/debug.js"></script>
<script language="Javascript" type="text/javascript">
var vdDebug = new voodooDebug({
	"debug_root": "<tmpl_var debug_root>",
	'app_id':     "<tmpl_var app_id>",
	'session_id': "<tmpl_var session_id>",
	'request_id': "<tmpl_var request_id>"
});
</script>

<div class="voodooDebug">
	<div class="vdClosed">
		<span onClick="vdDebug.handleSection(this,'top');"><img src="<tmpl_var debug_root>/i/plus.gif" width="9"/>Debug</span>
		<div class="vdContent">
			<a target="_new" href="http://debug.darkstar.nasba.int/index?app_id=<tmpl_var app_id>&session_id=<tmpl_var session_id>&request_id=<tmpl_var request_id>">Open Debug Window</a>
			<tmpl_if enable_profile>      <div class="vdClosed"><span onClick="vdDebug.handleSection(this,'profile');"      ><img src="<tmpl_var debug_root>/i/plus.gif" width="9"/>Generation Time</span><div id="vd_profile"       class="vdContent"></div></div></tmpl_if>
			<tmpl_if enable_anydebug>
				<div class="vdClosed">
					<span onClick="vdDebug.handleSection(this,'debug');"><img src="<tmpl_var debug_root>/i/plus.gif" width="9"/>Debug Trace</span>
					<div class="vdContent">
						<tmpl_if enable_debug>    [<span id="vd_switch_debug"     class="vdSwitchOn" onClick="vdDebug.toggleFilter(this,'debug')"    >debug</span>]</tmpl_if>
						<tmpl_if enable_info>     [<span id="vd_switch_info"      class="vdSwitchOn" onClick="vdDebug.toggleFilter(this,'info')"     >info</span>]</tmpl_if>
						<tmpl_if enable_warn>     [<span id="vd_switch_warn"      class="vdSwitchOn" onClick="vdDebug.toggleFilter(this,'warn')"     >warn</span>]</tmpl_if>
						<tmpl_if enable_error>    [<span id="vd_switch_error"     class="vdSwitchOn" onClick="vdDebug.toggleFilter(this,'error')"    >error</span>]</tmpl_if>
						<tmpl_if enable_exception>[<span id="vd_switch_exception" class="vdSwitchOn" onClick="vdDebug.toggleFilter(this,'exception')">exception</span>]</tmpl_if>
						<tmpl_if enable_trace>    [<span id="vd_switch_trace"     class="vdSwitchOn" onClick="vdDebug.toggleFilter(this,'trace')"    >trace</span>]</tmpl_if>
						<tmpl_if enable_table>    [<span id="vd_switch_table"     class="vdSwitchOn" onClick="vdDebug.toggleFilter(this,'table')"    >table</span>]</tmpl_if>
						<div id="vd_debug"></div>
					</div>
				</div>
			</tmpl_if>
			<tmpl_if enable_result>       <div class="vdClosed"><span onClick="vdDebug.handleSection(this,'return_data');"  ><img src="<tmpl_var debug_root>/i/plus.gif" width="9"/>Return Data</span    ><div id="vd_return_data"   class="vdContent"></div></div></tmpl_if>
			<tmpl_if enable_session>      <div class="vdClosed"><span onClick="vdDebug.handleSection(this,'session');"      ><img src="<tmpl_var debug_root>/i/plus.gif" width="9"/>Session Data</span   ><div id="vd_session"       class="vdContent"></div></div></tmpl_if>
			<tmpl_if enable_template_conf><div class="vdClosed"><span onClick="vdDebug.handleSection(this,'template_conf');"><img src="<tmpl_var debug_root>/i/plus.gif" width="9"/>Config Data</span    ><div id="vd_template_conf" class="vdContent"></div></div></tmpl_if>
			<tmpl_if enable_parameters>   <div class="vdClosed"><span onClick="vdDebug.handleSection(this,'parameters');"   ><img src="<tmpl_var debug_root>/i/plus.gif" width="9"/>Input Params</span   ><div id="vd_parameters"    class="vdContent"></div></div></tmpl_if>
		</div>
	</div>
</div>
