<!-- $Id$ -->
<link rel="stylesheet" type="text/css" media="screen" href="<tmpl_var debug_root>/style.css">
<script language="Javascript" type="text/javascript" src="<tmpl_var debug_root>/ajax.js"></script>
<script language="Javascript" type="text/javascript">
var vdDebug = new voodooDebug({
	"debug_root": "<tmpl_var debug_root>",
	'app_id':     "<tmpl_var app_id>",
	'session_id': "<tmpl_var session_id>",
	'request_id': "<tmpl_var request_id>"
});
</script>

<div class="voodooDebug">
	<div class="vdClosed">
		<span onClick="vdDebug.handleSection(this,'top');">Debug</span>
		<div class="vdContent">
			<div class="vdClosed"><span onClick="vdDebug.handleSection(this,'profile');"      >Generation Time</span   ><div id="vd_profile"       class="vdContent"></div></div>
			<div class="vdClosed"><span onClick="vdDebug.handleSection(this,'debug');"        >Debug Trace</span       ><div id="vd_debug"         class="vdContent"></div></div>
			<div class="vdClosed"><span onClick="vdDebug.handleSection(this,'return_data');"  >Template Variables</span><div id="vd_return_data"   class="vdContent"></div></div>
			<div class="vdClosed"><span onClick="vdDebug.handleSection(this,'session');"      >Session Variables</span ><div id="vd_session"       class="vdContent"></div></div>
			<div class="vdClosed"><span onClick="vdDebug.handleSection(this,'template_conf');">Config Variables</span  ><div id="vd_template_conf" class="vdContent"></div></div>
			<div class="vdClosed"><span onClick="vdDebug.handleSection(this,'parameters');"   >Parameters</span        ><div id="vd_parameters"    class="vdContent"></div></div>
			<div class="vdClosed"><span onClick="vdDebug.handleSection(this,'headers');"      >Request Headers</span   ><div id="vd_headers"       class="vdContent"></div></div>
		</div>
	</div>
</div>
