<!ELEMENT term_date           (#PCDATA)>
<!ELEMENT title               (#PCDATA)>
<!ELEMENT url                 (#PCDATA)>
<!ELEMENT user_reporting_date (#PCDATA)>
<!ELEMENT usernumber          (#PCDATA)>
<!ELEMENT zip                 (#PCDATA)>

<!ELEMENT course_level           (EMPTY)><!ATTLIST course_level           value (course_levels)        #REQUIRED>
<!ELEMENT delivery_mode          (EMPTY)><!ATTLIST delivery_mode          value (delivery_modes)       #REQUIRED>
<!ELEMENT license_type           (EMPTY)><!ATTLIST license_type           value (original|recriprocal) #REQUIRED>
<!ELEMENT principle_jurisdiction (EMPTY)><!ATTLIST principle_jurisdiction value (jurisdictions)        #REQUIRED>
<!ELEMENT program                (EMPTY)><!ATTLIST program                value (course_programs)      #REQUIRED>
<!ELEMENT report_odd_even        (EMPTY)><!ATTLIST report_odd_even        value (odd|even)             #REQUIRED>
<!ELEMENT state                  (EMPTY)><!ATTLIST state                  value (states)               #REQUIRED>
<!ELEMENT state_of_residence     (EMPTY)><!ATTLIST state_of_residence     value (states)               #REQUIRED>
<!ELEMENT time_zone              (EMPTY)><!ATTLIST time_zone              value (time_zones)           #REQUIRED>

